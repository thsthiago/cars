<div class="card">
  <img
    src="<%- carro.image %>"
    alt="Foto do <%- carro.nome %>"
    class="card__image"
  >

  <div class="card__descricao">
    <strong class="card__title"><%- carro.nome %></strong>
    <strong class="card__subtitle"><%- carro.info %></strong>

    <strong class="card__preco">
      <span>R$</span>
      6,000
    </strong>
  </div>
  <button class="card__btn" type="button">
    <a href="#">Mais detalhes</a>
  </button>
</div>
